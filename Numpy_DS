om.seed(1234)\n","print(rng)\n","print(rng.randn(10))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"-Xbh7JAVCXyq","executionInfo":{"status":"ok","timestamp":1649824085165,"user_tz":-330,"elapsed":368,"user":{"displayName":"Bhavana Hotchandani","userId":"10797006788106554736"}},"outputId":"1d44517b-3c3d-4db4-a5bf-1b807a209941"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["RandomState(MT19937)\n","[-0.48539085 -1.90515385 -0.5262218  -0.31354602  0.33070649  0.93853725\n"," -0.56821116 -0.19377527  1.31280484  1.681251  ]\n"]}]}]}